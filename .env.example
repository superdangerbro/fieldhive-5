# Firebase Configuration
FIREBASE_PROJECT_ID=fieldhive-5-0
FIREBASE_API_KEY=your-api-key-here
FIREBASE_AUTH_DOMAIN=fieldhive-5-0.firebaseapp.com
FIREBASE_STORAGE_BUCKET=fieldhive-5-0.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-sender-id-here
FIREBASE_APP_ID=your-app-id-here
FIREBASE_MEASUREMENT_ID=your-measurement-id-here

# Development Environment
NODE_ENV=development
REACT_APP_USE_EMULATOR=true

# Emulator Configuration
FIRESTORE_EMULATOR_HOST=localhost:8080
FIREBASE_AUTH_EMULATOR_HOST=localhost:9099
FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199
FUNCTIONS_EMULATOR_HOST=localhost:5001

# Voice Recognition Configuration
OPENAI_API_KEY=your-openai-api-key-here
GOOGLE_CLOUD_SPEECH_API_KEY=your-google-speech-api-key-here

# Mobile App Configuration
EXPO_BARCODE_SCANNER_ENABLED=true
EXPO_LOCATION_ENABLED=true
EXPO_SPEECH_ENABLED=true

# Web App Configuration
REACT_APP_MAP_API_KEY=your-map-api-key-here
REACT_APP_SENTRY_DSN=your-sentry-dsn-here

# Testing
JEST_WORKER_ID=1
TEST_FIREBASE_PROJECT_ID=fieldhive-5-0-test

# Production Environment (uncomment when deploying)
# NODE_ENV=production
# REACT_APP_USE_EMULATOR=false
